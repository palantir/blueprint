{
    "version": "2.0.0",
    "compilerOptions": {
        "declaration": true,
        "experimentalDecorators": true,
        "jsx": "react",
        "module": "commonjs",
        "moduleResolution": "node",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": false,
        "sourceMap": false,
        "stripInternal": true,
        "target": "es5",
        "typeRoots": [
            "../../../node_modules/@types"
        ]
    }
}
