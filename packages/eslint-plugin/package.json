{
    "name": "@blueprintjs/eslint-plugin",
    "version": "2.1.3",
    "description": "ESLint rules for use with @blueprintjs packages",
    "main": "lib/index.js",
    "scripts": {
        "clean": "rm -rf lib/*",
        "compile": "tsc -p src/",
        "lint": "run-p lint:es",
        "lint:es": "es-lint",
        "lint-fix": "es-lint --fix",
        "test": "ts-node --script-mode --pretty test/index.ts",
        "test:jest-ci": "jest --ci --runInBand"
    },
    "dependencies": {
        "@typescript-eslint/utils": "^5.21.0",
        "eslint": "^8.14.0"
    },
    "devDependencies": {
        "@blueprintjs/node-build-scripts": "^5.0.2",
        "@types/dedent": "^0.7.0",
        "dedent": "^0.7.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.6.2"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:palantir/blueprint.git",
        "directory": "packages/eslint-plugin"
    },
    "author": "Palantir Technologies",
    "license": "Apache-2.0"
}
