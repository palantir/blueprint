{
    "name": "@blueprintjs/eslint-plugin",
    "version": "2.4.1",
    "description": "ESLint rules for use with @blueprintjs packages",
    "main": "lib/index.js",
    "scripts": {
        "clean": "rm -rf lib/*",
        "compile": "tsc -p src/",
        "lint": "run-p lint:es",
        "lint:es": "es-lint",
        "lint-fix": "es-lint --fix",
        "test": "run-s test:compile test:run",
        "test:compile": "tsc -p test/",
        "test:run": "node build/test/index.js"
    },
    "dependencies": {
        "@typescript-eslint/utils": "^5.32.0",
        "eslint": "^8.21.0"
    },
    "devDependencies": {
        "@blueprintjs/node-build-scripts": "^5.1.0",
        "@tsconfig/node16": "^1.0.3",
        "@types/dedent": "^0.7.0",
        "dedent": "^0.7.0",
        "npm-run-all": "^4.1.5",
        "ts-node": "^10.9.1",
        "typescript": "^4.6.2"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:palantir/blueprint.git",
        "directory": "packages/eslint-plugin"
    },
    "author": "Palantir Technologies",
    "license": "Apache-2.0"
}
