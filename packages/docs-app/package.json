{
    "name": "@blueprintjs/docs-app",
    "version": "5.0.1",
    "description": "Blueprint Documentation Site",
    "private": true,
    "scripts": {
        "bundle": "webpack",
        "bundle:analyze": "webpack --profile --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json",
        "clean": "rm -rf dist/*",
        "dev": "webpack-dev-server",
        "dist": "cross-env NODE_ENV=production yarn bundle",
        "lint": "run-p lint:scss lint:es",
        "lint:scss": "sass-lint",
        "lint:es": "es-lint",
        "lint-fix": "es-lint --fix && sass-lint --fix",
        "test": "exit 0",
        "verify": "run-p dist lint"
    },
    "dependencies": {
        "@blueprintjs/core": "^5.0.1",
        "@blueprintjs/datetime": "^5.0.1",
        "@blueprintjs/datetime2": "^1.0.1",
        "@blueprintjs/docs-data": "^5.0.1",
        "@blueprintjs/docs-theme": "^5.0.1",
        "@blueprintjs/icons": "^5.0.0",
        "@blueprintjs/monaco-editor-theme": "^1.0.0",
        "@blueprintjs/select": "^5.0.1",
        "@blueprintjs/table": "^5.0.1",
        "@blueprintjs/test-commons": "^2.0.0",
        "@documentalist/client": "^4.0.0",
        "chroma-js": "^2.4.2",
        "classnames": "^2.3.1",
        "date-fns": "^2.28.0",
        "dedent": "^0.7.0",
        "downloadjs": "^1.4.7",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "monaco-editor": "^0.37.1",
        "normalize.css": "^8.0.1",
        "popper.js": "^1.16.1",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-transition-group": "^4.4.5",
        "tslib": "~2.5.0"
    },
    "devDependencies": {
        "@blueprintjs/node-build-scripts": "^8.0.0",
        "@blueprintjs/webpack-build-scripts": "^5.0.0",
        "@types/chroma-js": "~2.4.0",
        "@types/downloadjs": "~1.4.3",
        "@types/lodash": "~4.14.191",
        "copy-webpack-plugin": "^11.0.0",
        "monaco-editor-webpack-plugin": "^7.0.1",
        "npm-run-all": "^4.1.5",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.15.0"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:palantir/blueprint.git",
        "directory": "packages/docs-app"
    },
    "keywords": [
        "palantir",
        "blueprint",
        "docs"
    ],
    "author": "Palantir Technologies",
    "license": "Apache-2.0"
}
