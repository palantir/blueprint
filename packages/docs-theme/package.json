{
  "name": "@blueprintjs/docs-theme",
  "version": "2.1.2",
  "description": "Blueprint theme for documentalist",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/esm/index.d.ts",
  "style": "lib/css/docs-theme.css",
  "unpkg": "dist/docs-theme.bundle.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist/* && rm -rf lib/*",
    "compile": "run-p \"compile:*\"",
    "compile:esm": "tsc -p ./src",
    "compile:cjs": "tsc -p ./src/tsconfig.cjs.json",
    "compile:css": "sass-compile ./src",
    "dev": "run-p \"compile:esm -- --watch\" \"compile:css -- --watch\"",
    "dist": "run-s \"dist:*\"",
    "dist:bundle": "cross-env NODE_ENV=production webpack",
    "dist:css": "css-dist lib/css/*.css",
    "dist:verify": "assert-package-layout",
    "lint": "run-p lint:scss lint:ts",
    "lint:scss": "sass-lint",
    "lint:ts": "ts-lint",
    "lint-fix": "ts-lint --fix",
    "verify": "npm-run-all compile -p dist lint"
  },
  "dependencies": {
    "@blueprintjs/core": "^2.3.1",
    "@blueprintjs/select": "^2.0.1",
    "classnames": "^2.2",
    "documentalist": "^1.2.0",
    "fuzzaldrin-plus": "^0.5.0",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@blueprintjs/node-build-scripts": "^0.6.1",
    "@types/fuzzaldrin-plus": "^0.0.1",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-transition-group": "^2.2.1",
    "typescript": "~2.7.2",
    "webpack": "^3.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:palantir/blueprint.git"
  },
  "keywords": [
    "palantir",
    "blueprint",
    "docs",
    "theme",
    "documentalist"
  ],
  "author": "Palantir Technologies",
  "license": "Apache-2.0"
}
