        "test": "exit 0",
        "verify": "run-p dist lint"
    },
    "dependencies": {
        "@blueprintjs/core": "^4.0.0-beta.12",
        "@blueprintjs/datetime": "^4.0.0-beta.12",
        "@blueprintjs/icons": "^4.0.0-beta.6",
        "@blueprintjs/popover2": "^1.0.0-beta.12",
        "@blueprintjs/select": "^4.0.0-beta.12",
        "@blueprintjs/table": "^4.0.0-beta.12",
        "@blueprintjs/timezone": "^4.0.0-beta.12",
        "classnames": "^2.2",
        "core-js": "^3.8.1",
        "dom4": "^2.1.5",
        "lodash-es": "^4.17.15",
        "moment": "^2.29.1",
        "normalize.css": "^8.0.1",
