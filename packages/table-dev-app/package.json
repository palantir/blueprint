    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@blueprintjs/webpack-build-scripts": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "stylelint": "~9.6.0",
    "webpack": "^4.37.0",
    "webpack-dev-server": "^3.7.2"
  },
  "repository": {
    "type": "git",
