        "normalize.css": "^8.0.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@blueprintjs/webpack-build-scripts": "workspace:^",
        "@types/lodash": "~4.14.202",
        "copy-webpack-plugin": "^12.0.2",
        "npm-run-all": "^4.1.5",
        "webpack": "^5.90.0",
        "webpack-dev-server": "^4.15.1"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:palantir/blueprint.git",
        "directory": "packages/table-dev-app"
