{
  "name": "@blueprintjs/table-dev-app",
  "version": "0.2.0",
  "description": "Dev application for @blueprintjs/table",
  "private": true,
  "scripts": {
    "bundle": "webpack --config ./webpack.config.js",
    "clean": "rm -rf dist/*",
    "dev": "webpack-dev-server --config ./webpack.config.js",
    "dist": "NODE_ENV=production yarn bundle",
    "lint": "npm-run-all -p lint:stylelint lint:tslint",
    "lint:stylelint": "stylelint -s scss 'src/**/*.scss'",
    "lint:tslint": "tslint '{src,test}/**/*.{ts,tsx}' -c ../tslint-config -t codeFrame",
    "lint-fix": "yarn lint:tslint --fix",
    "test": "exit 0",
    "verify": "npm-run-all -p dist lint"
  },
  "dependencies": {
    "@blueprintjs/core": "^1.34.0",
    "@blueprintjs/table": "^1.31.0",
    "classnames": "^2.2.5",
    "dom4": "^1.8.3",
    "normalize.css": "^7.0.0",
    "react": "^16.2.0",
    "react-transition-group": "^2.2.1",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@blueprintjs/webpack-build-scripts": "^0.3.0",
    "npm-run-all": "^4.1.2",
    "stylelint": "^8.2.0",
    "tslint": "^5.8.0",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:palantir/blueprint.git"
  },
  "author": "Palantir Technologies",
  "license": "Apache-2.0"
}
