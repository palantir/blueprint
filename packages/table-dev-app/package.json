{
  "name": "@blueprintjs/table-dev-app",
  "version": "2.0.0",
  "description": "Dev application for @blueprintjs/table",
  "private": true,
  "scripts": {
    "bundle": "webpack --config ./webpack.config.js",
    "clean": "rm -rf dist/*",
    "dev": "webpack-dev-server --config ./webpack.config.js",
    "dist": "cross-env NODE_ENV=production yarn bundle",
    "lint": "npm-run-all -p lint:scss lint:ts",
    "lint:scss": "stylelint -s scss \"src/**/*.scss\"",
    "lint:ts": "tslint \"{src,test}/**/*.{ts,tsx}\" -t codeFrame",
    "lint-fix": "yarn lint:ts --fix",
    "test": "exit 0",
    "verify": "npm-run-all -p dist lint"
  },
  "dependencies": {
    "@blueprintjs/core": "^2.0.0",
    "@blueprintjs/table": "^2.0.0",
    "classnames": "^2.2.5",
    "dom4": "^2.0.1",
    "normalize.css": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-transition-group": "^2.2.1"
  },
  "devDependencies": {
    "@blueprintjs/webpack-build-scripts": "^0.5.0",
    "npm-run-all": "^4.1.2",
    "stylelint": "^8.4.0",
    "tslint": "^5.9.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:palantir/blueprint.git"
  },
  "author": "Palantir Technologies",
  "license": "Apache-2.0"
}
